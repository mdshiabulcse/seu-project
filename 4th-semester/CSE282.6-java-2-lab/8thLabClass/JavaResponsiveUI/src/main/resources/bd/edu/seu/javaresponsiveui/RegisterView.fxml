<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox   spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.javaresponsiveui.RegisterController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Welcome To JavaFx Login" />
    <Label />
    <Label prefHeight="17.0" prefWidth="34.0" text="Mail:" />
    <TextField fx:id="mail" />
    <Label text="Password:" />
    <TextField fx:id="password" />
    <CheckBox mnemonicParsing="false" text="Remember" />
    <Button layoutX="30.0" layoutY="269.0" onAction="#onHelloButtonClick" text="Forget Password" />
    <Button onAction="#onHelloButtonClick" text="Login!" />

    <Label fx:id="welcomeText" />
</VBox>
